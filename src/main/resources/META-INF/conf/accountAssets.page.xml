<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-2.0"
      name="My assets">
    <regions>
        <region>
            <list query-id="accountAsset">
                <content>
                    <left-top text-field-id="asset.ticker"/>
                    <left-bottom text-field-id="asset.name"/>
                    <right-top text-field-id="marketValue"/>
                    <right-bottom text-field-id="marketProfit"/>
                </content>
                <pagination/>
                <pre-filters>
                    <eq field-id="accountId" param="account_id"/>
                </pre-filters>
            </list>
        </region>
    </regions>

</page>
