<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0"
       name="Asset history">
    <list result-mapping="#this.assets" filters="accountId">
        <java method="getAccount" class="com.github.iryabov.invest.service.InvestService">
            <spring/>
            <arguments>
                <argument type="primitive" name="accountId"/>
            </arguments>
        </java>
    </list>
    <unique result-mapping="#this" filters="accountId,ticker">
        <java method="getAsset" class="com.github.iryabov.invest.service.InvestService">
            <spring/>
            <arguments>
                <argument type="primitive" name="accountId"/>
                <argument type="primitive" name="ticker"/>
            </arguments>
        </java>
    </unique>
</query>
