<?xml version='1.0' encoding='UTF-8'?>
<object xmlns="http://n2oapp.net/framework/config/schema/object-4.0">
    <fields>
        <field id="id" domain="integer"/>
        <field id="active" domain="boolean"/>
        <field id="account.id" mapping="['account_id']"/>
        <field id="sold_amount" mapping="['_sold_amount']"/>
        <field id="sold_quantity" mapping="['_sold_quantity']"/>
    </fields>

    <operations>
        <operation id="create">
            <invocation>
                <sql file="sql/dial_ins.sql"/>
            </invocation>
            <in>
                <field id="id"/>
                <field id="active"/>
                <field id="account.id"/>
                <field id="amount"/>
                <field id="currency"/>
                <field id="dt_close"/>
                <field id="dt_open"/>
                <field id="fee"/>
                <field id="note"/>
                <field id="sold_amount"/>
                <field id="sold_quantity"/>
            </in>
            <out>
                <field id="id" mapping="[0]"/>
            </out>
        </operation>
    </operations>
</object>
